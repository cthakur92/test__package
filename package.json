    "compile:scripts": "cross-env NODE_ENV=production babel --root-mode upward src  --out-dir dist ",
    "compile:styles": "node-sass src --include-path node_modules/ --include-path ../../node_modules/ --output dist",
    "compile:copy": "cpx src/**/*.{html,png,jpg} dist"
  },
